<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.komaki.a7segosr.ListPrintActivity">


    <ListView
        android:id="@+id/listview"
        android:layout_width="396dp"
        android:layout_height="377dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="6dp" />
    <Button
        android:id="@+id/uploadConfig"
        android:layout_width="165dp"
        android:layout_height="56dp"
        android:text="設定ファイルをアップロード"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/listview"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/downloadConfig"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.38" />

    <Button
        android:id="@+id/downloadConfig"
        android:layout_width="174dp"
        android:layout_height="53dp"
        android:text="設定ファイルをダウンロード"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/listview"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/deleteAll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="測定データを削除"
        tools:layout_editor_absoluteY="456dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
